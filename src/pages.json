{
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "%index.title%",
			"enablePullDownRefresh": true
		}
	}],
	"subPackages": [{
		"root": "pages/product/",
		"pages": [{
			"path": "detail",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTitleText": "详情",
				"enablePullDownRefresh": true
			}
		}]
	}, {
		"root": "pages/apis/",
		"pages": [{
			"path": "contact",
			"style": {
				"navigationBarTitleText": "联系人",
				"enablePullDownRefresh": true
			}
		}]
	}, {
		"root": "pages/demo/",
		"pages": [{
			"path": "language",
			"style": {
				"navigationBarTitleText": "国际化",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "lottery",
			"style": {
				"navigationBarTitleText": "抽奖",
				"enablePullDownRefresh": true
			}
		}]
	}],
	"preloadRule": {
		"pages/tab-bar/index": {
			"network": "all",
			"packages": ["__APP__"]
		},
		"pages/tab-bar/article": {
			"network": "all",
			"packages": ["__APP__"]
		},
		"pages/tab-bar/api": {
			"network": "all",
			"packages": ["__APP__"]
		},
		"pages/tab-bar/demo": {
			"network": "all",
			"packages": ["__APP__"]
		}
	},
	"uniIdRouter": {
		"loginPage": "pages/login/login",
		"needLogin": [
			"pages/user/.*"
		],
		"resToLogin": true
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"usingComponents": {
			"van-config-provider": "/wxcomponents/vant/config-provider/index",
			"van-transition": "/wxcomponents/vant/transition/index",
			"van-loading": "/wxcomponents/vant/loading/index",
			"van-row": "/wxcomponents/vant/row/index",
			"van-col": "/wxcomponents/vant/col/index",
			"van-cell": "/wxcomponents/vant/cell/index",
			"van-cell-group": "/wxcomponents/vant/cell-group/index",
			"van-popup": "/wxcomponents/vant/popup/index",
			"van-button": "/wxcomponents/vant/button/index",
			"van-icon": "/wxcomponents/vant/icon/index",
			"van-image": "/wxcomponents/vant/image/index",
			"van-field": "/wxcomponents/vant/field/index",
			"van-toast": "/wxcomponents/vant/toast/index",
			"van-nav-bar": "/wxcomponents/vant/nav-bar/index",
			"van-tabbar": "/wxcomponents/vant/tabbar/index",
			"van-tabbar-item": "/wxcomponents/vant/tabbar-item/index",
			"van-sidebar": "/wxcomponents/vant/sidebar/index",
			"van-sidebar-item": "/wxcomponents/vant/sidebar-item/index",
			"van-notice-bar": "/wxcomponents/vant/notice-bar/index"
		}
	}
}
